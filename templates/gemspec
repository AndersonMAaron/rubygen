Gem::Specification.new do |s|
  s.name        = ''
  s.version     = '0.0.1'
  s.date        = ''
  s.summary     = ''
  s.description = ''
  s.homepage    = ''
  s.authors     = []
  s.email       = ''

  s.executables << ''

  s.files       = Dir['lib/**/*.rb']

  s.add_development_dependency 'rake', '~> 10.4', '>= 10.4.2'
  s.add_development_dependency 'rspec', '~> 3.2'
  s.add_development_dependency 'simplecov', '~> 0.9', '>= 0.9.1'
end
